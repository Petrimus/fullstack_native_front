{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "lint": "eslint ./src/**/*.{js, jsx} App.js --no-error-on-unmatched-pattern",
    "test": "jest"
  },
  "jest": {
    "preset": "jest-expo",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@sentry/.*|react-router-native)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ]
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "apollo-boost": "^0.4.9",
    "expo": "^38.0.9",
    "expo-status-bar": "^1.0.2",
    "expo-web-browser": "~8.3.1",
    "formik": "^2.1.5",
    "graphql": "^15.3.0",
    "react": "~16.11.0",
    "react-dom": "~16.11.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-38.0.1.tar.gz",
    "react-native-paper": "^4.0.1",
    "react-native-web": "~0.11.7",
    "react-router-native": "^5.2.0",
    "use-debounce": "^3.4.3",
    "yup": "^0.29.2"
  },
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@testing-library/jest-native": "^3.3.0",
    "@testing-library/react-native": "^7.0.1",
    "babel-eslint": "^10.1.0",
    "babel-preset-expo": "~8.1.0",
    "eslint": "^7.5.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-react": "^7.20.5",
    "jest": "^26.2.2",
    "jest-expo": "^37.0.0",
    "react-native-dotenv": "^2.2.5"
  },
  "private": true
}
